<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name="productSpecification">
    <xs:sequence>
      <xs:element name="id" type="xs:string" minOccurs="0"/>
      <xs:element name="href" type="xs:string" minOccurs="0"/>
      <xs:element name="humanReadable" type="xs:string" minOccurs="0"/>
      <xs:element name="version" type="xs:string" minOccurs="0"/>
      <xs:element name="lastUpdate" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="name" type="xs:string" minOccurs="0"/>
      <xs:element name="description" type="xs:string" minOccurs="0"/>
      <xs:element name="validFor" type="timePeriod" minOccurs="0"/>
      <xs:element name="type" type="xs:string" minOccurs="0"/>
      <xs:element name="productSpecCharacteristics" type="specCharacteristics" minOccurs="0"/>
      <xs:element name="productSpecificationRelationships" type="productSpecificationRelationship" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="serviceSpecifications" type="serviceSpecification" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="resourceSpecifications" type="resourceSpecification" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="contractSpecifications" type="contractSpecification" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

</xs:schema>