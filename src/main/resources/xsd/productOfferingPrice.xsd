<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name="productOfferingPrice">
    <xs:sequence>
      <xs:element name="id" type="xs:string" minOccurs="0"/>
      <xs:element name="humanReadableId" type="xs:string" minOccurs="0"/>
      <xs:element name="name" type="xs:string" minOccurs="0"/>
      <xs:element name="description" type="xs:string" minOccurs="0"/>
      <xs:element name="type" type="xs:string" minOccurs="0"/>
      <xs:element name="subType" type="xs:string" minOccurs="0"/>
      <xs:element name="priceType" type="xs:string" minOccurs="0"/>
      <xs:element name="priceSubType" type="xs:string" minOccurs="0"/>
      <xs:element name="isCalculated" type="xs:boolean" minOccurs="0"/>
      <xs:element name="applyBeforeTax" type="xs:string" minOccurs="0"/>
      <xs:element name="isExternal" type="xs:boolean" minOccurs="0"/>
      <xs:element name="externalSystem" type="xs:string" minOccurs="0"/>
      <xs:element name="isPriced" type="xs:boolean" minOccurs="0"/>
      <xs:element name="price" type="price" minOccurs="0"/>
      <xs:element name="specCharacteristics" type="specCharacteristics" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="recurrencePeriod" type="recurrencePeriod" minOccurs="0"/>
      <xs:element name="unitOfMeasure" type="unitOfMeasure" minOccurs="0"/>
      <xs:element name="allowanceUnit" type="allowanceUnit" minOccurs="0"/>
      <xs:element name="productOfferingPriceAlteration" type="productOfferingPrice" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="discountTargets" type="discountTargets" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="conditionalPrices" type="conditionalPrices" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="productOfferingPriceCharacteristics" type="characteristic" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="personalizedPrice" type="personalizedPrice" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

</xs:schema>